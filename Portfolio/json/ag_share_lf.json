{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": {
    "text": "Share of the labour force employed in agriculture",
    "subtitle": "Source: Our World in Data",
    "anchor": "middle",
    "offset": 8,
    "subtitlePadding": 2
  },
  "data": {
    "url": "https://ourworldindata.org/grapher/share-of-the-labor-force-employed-in-agriculture.csv?v=1&csvType=filtered&useColumnShortNames=true&tab=line&time=1991..2019&country=CHN~AUS~GBR~IND&overlay=download-data",
    "format": {
      "type": "csv"
    }
  },
  "transform": [
    {
      "filter": "datum.Year >= 1990"
    }
  ],
  "mark": "line",
  "encoding": {
    "x": {
      "field": "Year",
      "type": "quantitative",
      "title": "Year",
      "axis": {
        "format": "d"
      }
    },
    "y": {
      "field": "share_employed_agri",
      "type": "quantitative",
      "title": "Labour force in agriculture (%)",
      "axis": {
        "grid": false
      }
    },
    "color": {
      "field": "Entity",
      "type": "nominal",
      "title": "Country"
    }
  },
  "height": 300,
  "width": "container",
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "padding": {
    "top": 56,
    "left": 56
  }
}
