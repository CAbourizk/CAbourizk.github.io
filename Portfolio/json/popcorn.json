{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": {
    "text": "Popcorn prices overtime",
    "subtitle": "Source: ONS microdata via Davies (2021)"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/CAbourizk/CAbourizk.github.io/refs/heads/main/Portfolio/csv/popcornprice.csv",
    "format": {
      "type": "csv"
    }
  },
  "mark": "line",
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "Year"
    },
    "y": {
      "field": "price",
      "type": "quantitative",
      "title": "Price (\u00a3)",
      "axis": {
        "grid": false
      }
    },
    "color": {
      "field": "price_type",
      "type": "nominal",
      "title": "Price Type"
    }
  },
  "width": 800,
  "height": 500
}
